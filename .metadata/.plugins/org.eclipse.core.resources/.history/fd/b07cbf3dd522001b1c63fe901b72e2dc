	function mobConv() {
	    var cn = new EN();
	    cn.setData("uid", "etland");
	    cn.setData("ordcode", ""); //옵션(태그 미삽입 시 모비온에서 발급되는 임의의 주문번호가 생성)
	    cn.setData("pcode", ""); //옵션
	    cn.setData("qty", ""); //필수
	    cn.setData("price", ""); //필수
	    cn.setData("pnm", encodeURIComponent(encodeURIComponent(""))); //옵션
	    cn.setSSL(true);
	    cn.sendConv();
	}

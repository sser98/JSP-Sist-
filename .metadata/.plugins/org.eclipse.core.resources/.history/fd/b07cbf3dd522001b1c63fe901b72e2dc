	//Enliple Common Tracker v3.6 [공용] start
	function mobRf() {
	    var rf = new EN();
 	    rf.setData("userid", "etland");
 	    rf.setSSL(true);
 	    rf.sendRf();
	}
	//Enliple Common Tracker v3.6 [공용] end

	function mobRfShop() {
 	    var sh = new EN();
 	    
 	    var price1 = "";
 	    var price2 = "";
 	    var price3 = "";
 	    var dcPrice = price3==0?price2:price3;
 	    dcPrice = dcPrice==0?price1:dcPrice;
 	    
 	    var soldOut = 2;
 	    soldOut= ""<=0?1:2;
 	    
 	    // [상품상세정보]
 	    sh.setData("sc", "8b33a1f99972451b9e3bb6a9e9ff8436");
 	    sh.setData("userid", "etland");
 	    sh.setData("pcode", "");
 	    sh.setData("price", "");
 	    sh.setData("pnm", encodeURIComponent(encodeURIComponent("")));
 	    sh.setData("img", encodeURIComponent("/nas/cdn/"));
 	    sh.setData("dcPrice", dcPrice); // 옵션
 	    sh.setData("soldOut", soldOut); //옵션 1:품절,2:품절아님
 	    sh.setData("cate1", encodeURIComponent(encodeURIComponent(""))); //필수
 	    sh.setSSL(true);
 	    sh.sendRfShop();

 	    // 장바구니 버튼 클릭 시 호출 메소드(사용하지 않는 경우 삭제)
 	    if(document.getElementById("cartBtn") != null){
 	    	document.getElementById("cartBtn").onmouseup = sendCart;
 	    	function sendCart() {
 	 	        sh.sendCart();
 	 	    }
 	    } 	  	
 	    
 	    // 찜,Wish 버튼 클릭 시 호출 메소드(사용하지 않는 경우 삭제)
 	    if(document.getElementById("wishBtn") != null){
	 	    document.getElementById("wishBtn").onmouseup = sendWish;
	 	    function sendWish() {
	 	        sh.sendWish();
	 	    }
 	    }
 	    /* // 네이버페이 버튼 컨버전 (사용하지 않는 경우 삭제)
	    sh.addEvent(document.querySelector("a[id^='NPAY_BUY_LINK_IDNC_ID']"), "click", enablePaySysConv);
 	    function enablePaySysConv(e) {
 	        sh.setData("qty", "수량");
 	        sh.enablePaySysConv(e);
 	    } */
	}
	

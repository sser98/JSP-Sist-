	    });

	    var $id = $("#MEM_MST_MEM_ID");
debugger;
		// 아이디 중복체크
		checkId = function(){
	        var value = $id.val();
debugger;

	        $("#checkId").val($id.val());	// 중복체크한 id

	        if(value == "")
	        {
	        	$("#memId").html("[아이디]는 필수 정보입니다.");
	            $id.focus();
	            return;
	        }
	        else
	        {
	        	var pattern1 = /^[a-z]+[a-z|0-9]{5,14}$/;
	        	var pattern2 = /[a-z]/g;
	        	var pattern3 = /[0-9]/g;

	        	if(!pattern1.test(value) || !pattern2.test(value) || !pattern3.test(value))
	            {
	        		$("#memIdCheck").html("사용할 수 없는 아이디입니다.\n영문과 숫자를 포함해서 입력해 주십시오.");
	        		$id.focus();
	                $("#idCheckYn").val("F");
	                return;
	            }
	            else
	            {
	            	$.ajax({
	                    async : false,
	                    type : "POST",
	                    data : {"MEM_MST_MEM_ID":$("#MEM_MST_MEM_ID").val()},
	                    url : "https://www.etlandmall.co.kr/pc/member/checkIdAjax.do",
	                    success : function (data) {
	                        if(data.successYn == "Y")
	                        {
	                            if(data.resultYn == "N")
	                            {
	                            	$("#memIdCheck").html("이미 사용중인 아이디입니다.");
	                            }
	                            else
	                            {
	                            	$("#memIdCheck").html("사용 가능한 아이디 입니다.");
	                            }

	                            $id.focus();
	                            $("#idCheckYn").val(data.resultYn);
	                            return;
	                        }
	                        else
	                        {
	                            alert("데이터 전송중 데이터가 누락 되었습니다.\n다시 시도해 주십시오.");
	                            return;
	                        }
	                    },
	                    error : function () {
	                        alert("데이터 오류 입니다. 다시 시도 해 주십시오.");
	                    }
	                });
	            }
	        }
	    };

 		// 이메일 도메인 select box 변경 이벤트
	    $(document).on("click", "#emailDomainSelectbox", function ()
	    {
	        var $domain = $("#MEM_MST_MAIL_DOMAIN");

	        if($(this).val() == "")
	        {
	            $domain.prop("readonly", false);
	            $domain.val("");
	        }
	        else
	        {
	            $domain.prop("readonly", true);
	            $domain.val($(this).val());
	            $("#memEmailDomain").html("");
	        }
	    });

	 	// 우편번호 찾기 클릭 이벤트
	    $(document).on("click","#zipCheck",function(){
	    	window.open("https://www.etlandmall.co.kr/pc/common/commonZipCodePopup.do","commonCodePopup","width=550,height=650,scrollbars=yes,target=_blank");
	    });

	 	// 우편번호 callback 함수
		zipcodeCallback = function (zip,doroAddr,jibunAddr)
		{
			$('#MEM_MST_ZIP_CDE5').val(zip);
			$('#MEM_MST_DRO_ADR1').val(doroAddr);
			$('#MEM_MST_JBN_ADR1').val(jibunAddr);
			$('#zip').html("");
			$('#doro').html("");
			$('#jibun').html("");
			$('#MEM_MST_ADR2').val("");
		};

	 	// 등록 버튼 클릭 이벤트
	    goRegist = function () {

	    	$('#MEM_MST_RST_YEAR').val($('input[type="checkbox"][name="RST_YEAR"]:checked').val());
	    	if($frm.valid()){

	 			var $chkObj = $("#idCheckYn");

	            //var $obj = $("#idCheck");

	            if($chkObj.val() == "Y")	// 중복체크 성공
	            {
	            	if($("#checkId").val() == $("#MEM_MST_MEM_ID").val()){		// 1. 중복체크 값과 ID필드 값이 같은 경우

	            		// 비밀번호와 아이디가 같은지 체크
	            		if($("#MEM_MST_MEM_ID").val() == $("#MEM_MST_PW").val()){
	            			$("#memPwCheck").html("아이디와 동일한 비밀번호는 사용하실 수 없습니다.");
	            			$("#MEM_MST_PW").focus();
	            			return;
	            		}

	            		//위메프 이벤트 4월17일 00시~4월 24일 11시  까지

	            		var rcmId = $("#MEM_MST_RCM_ID").val();
						var Now = new Date();
						var msg = "";
						var joinType = "memberBasic";

						var date =  Now.getDate();
						var hour = Now.getHours();

						if(rcmId == "위메프"&&joinType!="memberSimple")
						{
						  	if((date > 13 && date <  23) || (date==24 && hour <= 10))
						  	{
						  		msg +="                      개인정보 제3자 제공 동의\n\n";
						  		msg +="- 제공받는 자 : ㈜ 위메프\n";
						  		msg +="- 제공하는 개인정보 항목 : 성명, 휴대전화 번호\n";
						  		msg +="- 개인정보 이용기간 : 이벤트 경품 증정 업무 처리후 즉시 파기\n\n";
						  		msg +="위와 같이 경품 추첨 및 경품 증정 업무를 위해 “㈜ 위메프” 에\n";
						  		msg +="개인정보 제3자 제공에 동의 하십니까?\n";
						  		msg +="※ 미동의(취소) 시 이벤트에 참여하실 수 없습니다.\n";


						  		if(!confirm(msg))
						  		{
						  			return;
						  		}
						  	}
						}

// 						var cfrm = getConfirm();//추천사아이디가 있으면 추천판매사를 검증함.
			       		if (confirm( "저장 하시겠습니까?")) {
			       			if( $("#MEM_MST_RCM_ID") && $("#MEM_MST_RCM_ID").val() && $("#MEM_MST_RCM_ID").val().trim().length > 0){

			 					//임직원코드 체크
								if(staffTermsCheck()){

									//간편회원이면 alert
									if("memberBasic" == "memberSimple"){
										alert("임직원회원은 통합회원만 가입할 수 있습니다.\n임직원회원 가입을 원하지 않는경우\n임직원코드를 삭제해야 간편회원 가입이 가능합니다.");
									}else{
									//약관
										com.laypop('show',0,'layAgree');
									}
									return;
								}
		 					}

	            			// 이메일 ID + DOMAIN
		                    $("#MEM_MST_MAIL").val($("#MEM_MST_MAIL_ID").val() + "@" + $("#MEM_MST_MAIL_DOMAIN").val());

	            			com.laypop('show');//팝업 show

	            			$("#idCheckYn").val("");
	            			var url = "https://www.etlandmall.co.kr/pc/member/joinMemberRegist.do";
	            			if("" == 'LGT' ){
	            				url = "https://www.etlandmall.co.kr/ecm/pc/join/joinMemberRegist.do";
	            			}
	            			$('#Progress_Loading').show();
	    	                $frm.attr({"action":url, "target":"_self", "method":"post"}).submit();
			       		}


	            	}
	            	else{
	            		$("#memIdCheck").html("아이디 중복을 확인해 주십시오.");
	            		$id.focus();
	                    return;
	            	}
	            }
	            else if($chkObj.val() == "N")
	            {
	            	if($("#checkId").val() == $("#MEM_MST_MEM_ID").val()){
	            		$("#memIdCheck").html("사용할 수 없는 아이디입니다.");
	            		$id.focus();
			            return;
	            	}
	            	else{
	            		$("#memIdCheck").html("아이디 중복을 확인해 주십시오.");
	            		$id.focus();
	                    return;
	            	}
	            }
	            else if($chkObj.val() == "F")
	            {
	            	if($("#checkId").val() == $("#MEM_MST_MEM_ID").val()){
	            		$("#memIdCheck").html("사용할 수 없는 아이디입니다.");
	            		$id.focus();
	   	                return;
	            	}
	            	else{
	            		$("#memIdCheck").html("아이디 중복을 확인해 주십시오.");
	            		$id.focus();
	                    return;
	            	}
	            }
	            else{
	            	$("#memIdCheck").html("아이디 중복을 확인해 주십시오.");
	            	$id.focus();
                    return;
	            }
	        }else{
	 			//focus이동
	 			var _$errEl = $("#frm").find('input.error')[0].closest('div.inp_box');
	 			if($(_$errEl).find('input'))  $(_$errEl).find('input').focus();
	 		}



	    };


    	//임직원 코드 유효성 체크
    	var staffTermsCheck = function(){
			var result = "";

			$.ajax({
	               async : false,
	               type : "POST",
	               data : {"MEM_MST_RCM_ID":$("#MEM_MST_RCM_ID").val().trim()},
	               url : "https://www.etlandmall.co.kr/pc/mypage/selectStaffMaster.do",
	               success : function (data) {
	                   if(data.staffCodeCount > 0)
	                   {
	                	   result =  true;
	                   }else{
	                	   result = false;
	                   }
	               },
	               error : function () {
	                   alert("데이터 오류 입니다. 다시 시도 해 주십시오.");
	               }
	           });




			return result;
    	};

	 	// 취소 클릭 이벤트
	    goCancel = function(){
	    	if(confirm("회원가입을 취소하시겠습니까?")){
	 			location.href = "http://www.etlandmall.co.kr/pc/main/index.do";
	 		}
	    };

	    // 아이디 및 비밀번호의 체크 메세지 초기화
	    deleteCheckMsg = function(){
	    	$("#memIdCheck").html("");
	    	$("#memPwCheck").html("");
	    };

	    getConfirm = function(){
	    	var msg = "저장 하시겠습니까?";
	    	if($("#MEM_ECM_RCM_ID").val()){

				$.ajax({
			       	type: "POST",
			        async: false,
			        data : {
			        			"ECM_MST_IDX"		: '1'//Uplus
			            		, "ECM_REC_DATA_ECH_NO" 	: $("#MEM_ECM_RCM_ID").val()
			        		},
			        url: "/ecm/mobile/join/checkEcmRecId.do",
			        dataType: "json",
			        success: function(json) {
			        	if(json.data){
				        	if(json.data.result==0){//없는 코드
				        		alert(json.data.failMsg);
				        		$("#MEM_ECM_RCM_ID").val("");
				        		cfrm = false;
				        		return;
				        	}else{
				        		$("#MEM_ECM_RCM_ID_VLD").val("Y");
					        	//추천 판매사 정보 표기하기
					        	if(json.data.result==1){
					        		cfrm = confirm("[ "+json.data.ECM_REC_DATA_ECH_NM + " ] 추천 판매사가 맞습니까")
					        		$("#TRM_YN0009").val("Y");

					        	}else{
					        		//console.log("추천 판매코드는 있지만, 추천 판매사 상세 정보없음. 바로 저장.")
					        		$("#TRM_YN0009").val("Y");
					        		cfrm = true;
					        	}
				        	}
			        	}else{
							if(("#MEM_ECM_RCM_ID_VLD").val() == 'N'){
								alert(("#MEM_ECM_RCM_ID").val()+"추천 판매사 정보가 없습니다./n다시 입력하시거나 공란으로 진행해주세요.");
								("#MEM_ECM_RCM_ID").val("");
								cfrm = false;
				        		return;
							}
			        	}
			        },
			        error: function(xhr,status,error) {
			        	alert("추천 판매사 코드 호출 중 에러가 발생하였습니다.");
			        	cfrm = false;
			        	("#MEM_ECM_RCM_ID").val("");
		        		return;
			        }
			    });

			}else{

				cfrm = confirm(msg);
			}

			return cfrm;
	    };

	});
</script>

